run = "cd backend && python main.py"
entrypoint = "backend/main.py"

[nix]
channel = "stable-22_11"

[env]
PYTHONPATH = "${REPL_HOME}/backend"

[packager]
language = "python3"

[packager.features]
packageSearch = true
guessImports = true

[languages]
[languages.python]
pattern = "**/*.py"
syntax = "python"

[languages.javascript]
pattern = "**/*.js"
syntax = "javascript" 